{
    "name": "akutaktau/CakeSocial",
    "description": "CakeSocial plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "facebook/php-sdk-v4": "~5.0",
        "abraham/twitteroauth": "^0.7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
	
    "autoload": {
        "psr-4": {
            "CakeSocial\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeSocial\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
